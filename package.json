    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^6.0.0",
    "ethers": "^4.0.27",
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "human-standard-token-abi": "^2.0.0",
    "js-sha256": "^0.9.0",
    "prettier": "^1.15.3",
    "request": "^2.88.0",
